							"raw": "{\"index\":{\"_id\":\"2\"}}\n{\"id\":\"2\",\"name\":\"iPhone 15 Pro\",\"description\":\"Latest Apple smartphone with A17 Pro chip and advanced camera system\",\"price\":999.99,\"category\":\"Electronics\",\"stock\":50,\"tags\":[\"smartphone\",\"apple\",\"iphone\",\"mobile\"],\"createdDate\":\"2025-11-25T10:00:00Z\",\"isActive\":true}\n{\"index\":{\"_id\":\"3\"}}\n{\"id\":\"3\",\"name\":\"Samsung Galaxy S24\",\"description\":\"Powerful Android phone with AI features and excellent display\",\"price\":899.99,\"category\":\"Electronics\",\"stock\":40,\"tags\":[\"smartphone\",\"samsung\",\"android\",\"galaxy\"],\"createdDate\":\"2025-11-25T10:00:00Z\",\"isActive\":true}\n{\"index\":{\"_id\":\"4\"}}\n{\"id\":\"4\",\"name\":\"Sony WH-1000XM5\",\"description\":\"Premium noise-cancelling wireless headphones\",\"price\":399.99,\"category\":\"Audio\",\"stock\":30,\"tags\":[\"headphones\",\"sony\",\"wireless\",\"noise-cancelling\"],\"createdDate\":\"2025-11-25T10:00:00Z\",\"isActive\":true}\n{\"index\":{\"_id\":\"5\"}}\n{\"id\":\"5\",\"name\":\"iPad Pro 12.9\",\"description\":\"Professional tablet with M2 chip and Liquid Retina display\",\"price\":1099.99,\"category\":\"Electronics\",\"stock\":20,\"tags\":[\"tablet\",\"apple\",\"ipad\",\"professional\"],\"createdDate\":\"2025-11-25T10:00:00Z\",\"isActive\":true}\n{\"index\":{\"_id\":\"6\"}}\n{\"id\":\"6\",\"name\":\"MacBook Air M3\",\"description\":\"Ultra-thin laptop with powerful M3 chip and all-day battery life\",\"price\":1299.99,\"category\":\"Electronics\",\"stock\":15,\"tags\":[\"laptop\",\"apple\",\"macbook\",\"lightweight\"],\"createdDate\":\"2025-11-25T10:00:00Z\",\"isActive\":true}\n{\"index\":{\"_id\":\"7\"}}\n{\"id\":\"7\",\"name\":\"Logitech MX Master 3S\",\"description\":\"Ergonomic wireless mouse for productivity\",\"price\":99.99,\"category\":\"Accessories\",\"stock\":45,\"tags\":[\"mouse\",\"logitech\",\"wireless\",\"ergonomic\"],\"createdDate\":\"2025-11-25T10:00:00Z\",\"isActive\":true}\n{\"index\":{\"_id\":\"8\"}}\n{\"id\":\"8\",\"name\":\"Bose QuietComfort Earbuds II\",\"description\":\"True wireless earbuds with world-class noise cancellation\",\"price\":279.99,\"category\":\"Audio\",\"stock\":35,\"tags\":[\"earbuds\",\"bose\",\"wireless\",\"noise-cancelling\"],\"createdDate\":\"2025-11-25T10:00:00Z\",\"isActive\":true}\n{\"index\":{\"_id\":\"9\"}}\n{\"id\":\"9\",\"name\":\"USB-C Cable 3-Pack\",\"description\":\"Fast charging USB-C cables, 6ft length\",\"price\":19.99,\"category\":\"Accessories\",\"stock\":100,\"tags\":[\"cable\",\"usb-c\",\"charging\",\"fast-charge\"],\"createdDate\":\"2025-11-25T10:00:00Z\",\"isActive\":true}\n{\"index\":{\"_id\":\"10\"}}\n{\"id\":\"10\",\"name\":\"Samsung 27 inch 4K Monitor\",\"description\":\"Ultra HD monitor with HDR support for professionals\",\"price\":449.99,\"category\":\"Electronics\",\"stock\":12,\"tags\":[\"monitor\",\"samsung\",\"4k\",\"display\"],\"createdDate\":\"2025-11-25T10:00:00Z\",\"isActive\":true}\n"
						},
						"url": {
							"raw": "{{es_host}}/products/_bulk?refresh=wait_for",
							"host": ["{{es_host}}"],
							"path": ["products", "_bulk"],
							"query": [{"key": "refresh", "value": "wait_for"}]
