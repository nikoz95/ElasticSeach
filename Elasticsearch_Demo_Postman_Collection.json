						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"id\": \"1\",\n  \"name\": \"Laptop Dell XPS 15\",\n  \"description\": \"High-performance laptop with Intel Core i7 processor and 16GB RAM\",\n  \"price\": 1499.99,\n  \"category\": \"Electronics\",\n  \"stock\": 25,\n  \"tags\": [\"laptop\", \"computer\", \"dell\", \"premium\"],\n  \"createdDate\": \"2025-11-25T10:00:00Z\",\n  \"isActive\": true\n}"
						},
						"url": {
							"raw": "{{es_host}}/products/_doc/1?refresh=wait_for",
							"host": ["{{es_host}}"],
							"path": ["products", "_doc", "1"],
