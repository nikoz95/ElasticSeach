		{
			"name": "5. Basic Queries",
			"item": [
				{
					"name": "Match All Query",
					"request": {
						"method": "GET",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"query\": {\n    \"match_all\": {}\n  }\n}"
						},
						"url": {
							"raw": "{{es_host}}/products/_search?pretty=true",
							"host": ["{{es_host}}"],
							"path": ["products", "_search"],
							"query": [{"key": "pretty", "value": "true"}]
						}
					}
				},
				{
					"name": "Match Query (Full-Text)",
					"request": {
						"method": "GET",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"query\": {\n    \"match\": {\n      \"description\": \"laptop processor\"\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{es_host}}/products/_search?pretty=true",
							"host": ["{{es_host}}"],
							"path": ["products", "_search"],
							"query": [{"key": "pretty", "value": "true"}]
						}
					}
				},
				{
					"name": "Multi-Match Query",
					"request": {
						"method": "GET",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"query\": {\n    \"multi_match\": {\n      \"query\": \"apple\",\n      \"fields\": [\"name\", \"description\"]\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{es_host}}/products/_search?pretty=true",
							"host": ["{{es_host}}"],
							"path": ["products", "_search"],
							"query": [{"key": "pretty", "value": "true"}]
						}
					}
				},
				{
					"name": "Term Query (Exact Match)",
					"request": {
						"method": "GET",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"query\": {\n    \"term\": {\n      \"category\": \"Electronics\"\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{es_host}}/products/_search?pretty=true",
							"host": ["{{es_host}}"],
							"path": ["products", "_search"],
							"query": [{"key": "pretty", "value": "true"}]
						}
					}
				},
				{
					"name": "Terms Query (Multiple Values)",
					"request": {
						"method": "GET",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"query\": {\n    \"terms\": {\n      \"category\": [\"Electronics\", \"Audio\"]\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{es_host}}/products/_search?pretty=true",
