							"path": ["products", "_search"],
							"query": [{"key": "pretty", "value": "true"}]
						}
					}
				},
				{
					"name": "Range Query (Price)",
					"request": {
						"method": "GET",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"query\": {\n    \"range\": {\n      \"price\": {\n        \"gte\": 100,\n        \"lte\": 500\n      }\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{es_host}}/products/_search?pretty=true",
							"host": ["{{es_host}}"],
							"path": ["products", "_search"],
							"query": [{"key": "pretty", "value": "true"}]
						}
					}
				},
				{
					"name": "Bool Query (Combined)",
					"request": {
						"method": "GET",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"query\": {\n    \"bool\": {\n      \"must\": [\n        {\"term\": {\"category\": \"Electronics\"}}\n      ],\n      \"filter\": [\n        {\"range\": {\"price\": {\"gte\": 500, \"lte\": 1500}}}\n      ]\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{es_host}}/products/_search?pretty=true",
							"host": ["{{es_host}}"],
							"path": ["products", "_search"],
							"query": [{"key": "pretty", "value": "true"}]
						}
					}
				},
