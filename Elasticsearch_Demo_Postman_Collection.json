		{
			"name": "2. Index Management",
			"item": [
				{
					"name": "Create Products Index",
					"request": {
						"method": "PUT",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"settings\": {\n    \"number_of_shards\": 1,\n    \"number_of_replicas\": 1,\n    \"analysis\": {\n      \"analyzer\": {\n        \"custom_analyzer\": {\n          \"type\": \"custom\",\n          \"tokenizer\": \"standard\",\n          \"filter\": [\"lowercase\", \"stop\"]\n        }\n      }\n    }\n  },\n  \"mappings\": {\n    \"properties\": {\n      \"id\": {\"type\": \"keyword\"},\n      \"name\": {\n        \"type\": \"text\",\n        \"fields\": {\n          \"keyword\": {\"type\": \"keyword\"}\n        }\n      },\n      \"description\": {\"type\": \"text\"},\n      \"price\": {\"type\": \"scaled_float\", \"scaling_factor\": 100},\n      \"category\": {\"type\": \"keyword\"},\n      \"stock\": {\"type\": \"integer\"},\n      \"tags\": {\"type\": \"keyword\"},\n      \"createdDate\": {\"type\": \"date\"},\n      \"isActive\": {\"type\": \"boolean\"}\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{es_host}}/products",
							"host": ["{{es_host}}"],
							"path": ["products"]
						}
					}
				},
				{
					"name": "Create Articles Index",
					"request": {
						"method": "PUT",
						"header": [{"key": "Content-Type", "value": "application/json"}],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"settings\": {\n    \"number_of_shards\": 1,\n    \"number_of_replicas\": 1\n  },\n  \"mappings\": {\n    \"properties\": {\n      \"id\": {\"type\": \"keyword\"},\n      \"title\": {\n        \"type\": \"text\",\n        \"fields\": {\n          \"keyword\": {\"type\": \"keyword\"}\n        }\n      },\n      \"content\": {\"type\": \"text\"},\n      \"author\": {\"type\": \"keyword\"},\n      \"keywords\": {\"type\": \"keyword\"},\n      \"publishDate\": {\"type\": \"date\"},\n      \"viewCount\": {\"type\": \"integer\"}\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{es_host}}/articles",
							"host": ["{{es_host}}"],
							"path": ["articles"]
						}
					}
				},
